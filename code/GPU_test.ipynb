    "import tensorflow as tf\n",
    "with tf.device('/gpu:0'):\n",
    "    a = tf.constant([1.0,2.0,3.0],shape=[1,3],name='a')\n",
    "    b = tf.constant([1.0,2.0,3.0],shape=[3,1],name='b')\n",
    "    c = tf.matmul(a,b)\n",
    "    \n",
    "with tf.Session() as sess:\n",
    "    print(sess.run(c))"
